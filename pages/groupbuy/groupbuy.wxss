  transform: translateX(-50%);
  border-radius: 16rpx;
}
.ptPop > .btn {
  width: 480rpx;
  height: 72rpx;
  border-radius: 36rpx;
  color: #fff;
  font-size: 28rpx;
  font-weight: 600;
  margin-bottom: 20rpx;
}

