/* 分享按钮 */
.share {
  width: 72rpx;
  height: 72rpx;
  border-radius: 50%;
  position: absolute;
  background-color: #d8d8d8;
  top: 30rpx;
  right: 30rpx;
  z-index: 999;
  text-align: center;
  line-height: 72rpx;
  font-size: 60rpx;
  color: #fff;
}

/* 轮播图区域 */
.swiper {
  width: 750rpx;
  height: 750rpx;
  background-color: #ccc;
