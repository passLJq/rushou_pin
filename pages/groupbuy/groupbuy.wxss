  width: 160rpx;
  height: 60rpx;
  border-radius: 60rpx;
  -webkit-border-radius: 60rpx;
  font-size: 24rpx;
  text-align: center;
  line-height: 60rpx;
  color: #fff;
}

/* 拼团弹窗 */
.ptPop {
  width: 570rpx;
  border-radius: 16rpx;
