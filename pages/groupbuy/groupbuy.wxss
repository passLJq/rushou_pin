/* 拼团成员 */
.member {
  width: 750rpx;
  background-color: #fff;
  padding: 0 40rpx;
  margin-top: 20rpx;
}
.member .member-item {
  width: 100%;
  height: 140rpx;
  align-items: center;
}
.member .member-item + .member-item {
  border-top: 1rpx solid #f6f6f6;
}
.member .member-item image {
  width: 96rpx;
  height: 96rpx;
  border-radius:50%;
  -webkit-border-radius:50%;
  margin-right: 20rpx;
}
.member .member-item view {
  height: 86rpx;
}
.member .member-item view text {
  font-size: 28rpx;
  max-width: 390rpx;
}
.member .member-item view text:nth-child(2) {
